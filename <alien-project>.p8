--player
function create_player()
p={x=10, y=5, sprite=3}
end



function player_movement()
 newx=p.x
 newy=p.y
if (btnp(➡️)) newx+=1
if (btnp(⬅️)) newx-=1

 if not check_flag(0,newx,newy) then
 p.x = newx
 p.y = newy
 end
end

function draw_player()
 spr(p.sprite,p.x*8,p.y*8)
end



